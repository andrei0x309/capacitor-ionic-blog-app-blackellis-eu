{
  "name": "blackellis-eu-mobile",
  "version": "0.0.1",
  "private": true,
  "description": "Ionic Vue blackellis.eu Mobile",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src&&prettier . -w -u",
    "build": "vue-tsc --noEmit && vite build && bun x cap sync",
    "apply-patches": "bun run ./scripts/apply-patches.ts",
    "android": "ionic cap run android -l --external --host=4875.flashsoft.eu --port=4875 --livereload-url=https://4875.flashsoft.eu",
    "android-open": "ionic capacitor open android",
    "android-res": "cordova-res android --skip-config --copy",
    "android-ver": "deno run -A ./scripts/update-version.ts",
    "android-pub": "deno run -A ./scripts/publish-preview.ts",
    "android-res-new": "npx capacitor-assets generate --android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.2",
    "@capacitor/background-runner": "^2.1.0",
    "@capacitor/core": "^7.4.2",
    "@capacitor/device": "^7.0.2",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/keyboard": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/share": "^7.0.2",
    "@capacitor/splash-screen": "^7.0.2",
    "@capacitor/status-bar": "^7.0.2",
    "@ionic/storage": "^4.0.0",
    "@ionic/vue": "^8.7.2",
    "@ionic/vue-router": "^8.7.2",
    "ionicons": "^8.0.13",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.4.2",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "cypress": "^14.5.4",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "^10.4.0",
    "jest": "^30.0.5",
    "sass": "^1.90.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vue-tsc": "^3.0.5",
    "@tailwindcss/vite": "^4.1.12",
    "tailwindcss": "^4.1.12"
  },
  "browser": {
    "stream": false
  }
}